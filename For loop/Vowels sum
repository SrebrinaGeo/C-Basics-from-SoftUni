#include <iostream>
#include <string>
using namespace std;

int main()
{
    string text; getline(cin, text);
    int sum = 0;

    for (int index = 0; index <= text.length() - 1; index++)
    {
        char currentSymbol = text[index];

        switch (currentSymbol)
        {
        case 'a':
            sum += 1;
            break;
        case 'e':
            sum += 2;
            break;
        case 'i':
            sum += 3;
            break;
        case 'o':
            sum += 4;
            break;
        case 'u':
            sum += 5;
            break;
        }
    }
       
    cout << sum << endl;
    return 0;
}