#include <iostream>
using namespace std;

int main()
{
	
	double budget; cin >> budget;
	string season, destination, type; cin >> season;
	double sum;

	if (budget <= 100)
	{
		destination = "Bulgaria";
	
		if (season == "summer")
		{
			type = "Camp";			
			sum = budget * 0.3;
		}

		else if (season == "winter")
		{
			type = "Hotel";
			sum = budget * 0.7;
		}
	}

	else if (budget <= 1000)
	{
		destination = "Balkans";

		if (season == "summer")
		{
			type = "Camp";
			sum = budget * 0.4;
		}

		else if (season == "winter")
		{
			type = "Hotel";
			sum = budget * 0.8;
		}
	}

	else if (budget > 1000)
	{
		destination = "Europe";
		type = "Hotel";

		sum = budget * 0.9;
	}

		
	cout <<  "Somewhere in " << destination << endl;
	
	cout.setf(ios::fixed);
	cout.precision(2);
	cout << type << " - " << sum << endl;
	

	return 0;
}