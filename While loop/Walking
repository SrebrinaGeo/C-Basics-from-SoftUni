#include <iostream>
#include <string>
using namespace std;

int main()
{
    const int stepsGoal = 10000;
    int sum = 0;
    bool goalIsReached = false;

    string input;

    while (getline(cin, input) && input != "Going home")
    {
        int currentSteps = stoi(input);

        sum += currentSteps;

        if (sum >= stepsGoal)
        {
            goalIsReached = true;
            break;
        }
    }

    if (!goalIsReached)
    {
        int stepsHome;
        cin >> stepsHome;

        sum += stepsHome;
        if (sum >= stepsGoal)
        {
            goalIsReached = true;
        }
    }
    
    if (goalIsReached)
    {
        cout << "Goal reached! Good job!" << endl;
    }
    else
    {
        cout << stepsGoal - sum << " more steps to reach goal." << endl;
    }
    return 0;
}