#include <iostream>
using namespace std;

int main()
{
    int count1lv, count2lv, count5lv, sum;
    cin >> count1lv >> count2lv >> count5lv >> sum;

    for (int i = 0; i <= count1lv; i++)
    {
        for (int j = 0; j <= count2lv; j++)
        {
            for (int k = 0; k <= count5lv; k++)
            {

                if (i * 1 + j * 2 + k * 5 == sum)
                {
                    cout << i << " * 1 lv. + "  << j << " * 2 lv. + " << k << " * 5 lv. = " << sum << " lv." << endl;
                }
            }
        }
    }

    return 0;
}
