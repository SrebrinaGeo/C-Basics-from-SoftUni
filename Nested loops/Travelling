#include <iostream>
#include <string>
#include <cstdlib> 
using namespace std;

int main()
{
	string input;
	cin >> input;

	while (input != "End")
	{
		string destination = input;
		double budget;
		cin >> budget;

		double savedMoney = 0;

		while (savedMoney < budget)
		{
			double sum;
			cin >> sum;
			savedMoney += sum;
		}

		cout << "Going to " << destination << "!" << endl;

		cin >> input;
	}
	return 0;
}
